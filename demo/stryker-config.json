{
  "stryker-config":
  {
    "solution": "F0.Talks.MutationTesting.sln",
    "project": "F0.Talks.MutationTesting.csproj",
    "test-projects": [ "F0.Talks.MutationTesting.Tests/F0.Talks.MutationTesting.Tests.csproj" ],
    "mutate": [
      "!Exceptions/Thrower.cs",
      "!**/*Throw*.cs"
    ],
    "target-framework": "net8.0",
    "mutation-level": "Advanced",
    "reporters": [
      "html",
      "cleartext",
      "dots",
      "markdown"
    ],
    "additional-timeout": 10000,
    "thresholds": {
      "high": 100,
      "low": 90,
      "break": 80
    },
    "ignore-mutations": [
      "logical",
      "linq.First",
      "linq.Sum"
    ],
    "ignore-methods": [
      "*Log*",
      "ToString",
      "*HashCode*",
      "Assert",
      "ConfigureAwait",
      "NegativeNumberException.ctor",
      "*Exception.ctor",
      "Console.Write*"
    ],
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "disable-mix-mutants": false,
    "since": {
      "enabled": false,
      "target": "main",
      "ignore-changes-in": []
    },
    "verbosity": "info",
    "break-on-initial-test-failure": true
  }
}
