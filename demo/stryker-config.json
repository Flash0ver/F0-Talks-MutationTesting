{
  "stryker-config":
  {
    "reporters": [
      "dots",
      "cleartext",
      "json",
      "html"
    ],
    "verbosity": "info",
    "additional-timeout": 10000,
    "solution": "F0.Talks.MutationTesting.sln",
    "project": "F0.Talks.MutationTesting.csproj",
    "test-projects": [ "F0.Talks.MutationTesting.Tests/F0.Talks.MutationTesting.Tests.csproj" ],
    "concurrency": 4,
    "thresholds": {
      "high": 100,
      "low": 90,
      "break": 80
    },
    "mutation-level": "Standard",
    "mutate": [
      "!Exceptions/Thrower.cs",
      "!**/*Throw*.cs"
    ],
    "ignore-mutations": [
      "logical"
    ],
    "ignore-methods": [
      "*Log*",
      "ToString",
      "*HashCode*",
      "Assert",
      "ConfigureAwait",
      "NegativeNumberException.ctor",
      "*Exception.ctor"
    ],
    "coverage-analysis": "perTest",
    "disable-bail": false,
    "since": {
      "enabled": false,
      "target": "main",
      "ignore-changes-in": []
    }
  }
}
