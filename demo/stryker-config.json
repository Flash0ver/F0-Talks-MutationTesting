{
  "stryker-config":
  {
    "reporters": [
      "dots",
      "cleartext",
      "json",
      "html"
    ],
    "log-level": "info",
    "log-file": true,
    "timeout-ms": 10000,
    "project-file": "F0.Talks.MutationTesting.csproj",
    "test-projects": null,
    "max-concurrent-test-runners": 4,
    "threshold-high": 100,
    "threshold-low": 90,
    "threshold-break": 80,
    "mutation-level": "Standard",
    "mutate": [
      "!Exceptions/Thrower.cs",
      "!**/*Throw*.cs"
    ],
    "excluded-mutations": [
      "logical"
    ],
    "ignore-methods": [
      "*Log*",
      "ToString",
      "*HashCode*",
      "Assert",
      "ConfigureAwait",
      "NegativeNumberException.ctor",
      "*Exception.ctor"
    ],
    "coverage-analysis": "perTest",
    "abort-test-on-fail": true,
    "diff": false,
    "diff-ignore-files": [],
    "git-diff-target": "main"
  }
}
